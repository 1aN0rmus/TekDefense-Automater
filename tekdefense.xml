        <sitefriendlyname>
	        <entry>VT pDNSIP</entry>
	        <entry>VT pDNSURL</entry>
	    </sitefriendlyname>
        <regex>
		    <!--<entry>\w{3}</entry>-->
	        <entry>(\d{4}\-\d{1,2}\-\d{1,2})\s+&lt;.{30,70}\/en\/ip-address\/(.{1,80})\/information</entry>
	        <entry>(\d{4}\-\d{1,2}\-\d{1,2}).{1,20}\s+&lt;.{10,80}\/en\/url\/.{1,100}\/analysis\/.{1,5}\s+(http.{1,70}\/)</entry>
